<body>
	
	<script type="text/javascript">
		/*
		--------------------------------------------------------------
		function: 	CreateSiebelObject
		arguments: 	none
		returns:	none
		desc.:		Create new Siebel ActiveX object for manipulation.
		--------------------------------------------------------------
		*/
		function SetSiebelObject() {
			var bSiebelExists = false;
			try {
				if (CheckSiebelExe()){
					if (oSiebel == null) {
						oSiebel = new ActiveXObject("TWSiebel.SiebelWebApplication.1");
						UpdateStatus ("Connected to Siebel..");
						SetBasicInfo();
					}
					bSiebelExists = true;
				}
				else {
					oSiebel = null;
					UpdateStatus("SiTrace needs Siebel dedicated/mobile client (siebel.exe) to be running. Validate whether siebel.exe is running on the computer, and retry.")
				}
				return bSiebelExists;
			}
			catch(e){
				throw(e);
			}
		}
		
		/*
		--------------------------------------------------------------
		function: 	ChangeTrace
		arguments: 	none
		returns:	none
		desc.:		Turns on/off the Siebel tracing with params
					selected on the UI.
		--------------------------------------------------------------
		*/
		function ChangeTrace() {
			var sTraceOption = "Allocation", sTraceAll = "All";
			if (SetSiebelObject()) {
				if (document.getElementById("btnTrace").value == "Trace On" && oSiebel != null) {
					if (document.getElementsByName("optTrace")[1].checked) {
						sTraceOption = "SQL";
						sTraceAll = "";
					}
					oSiebel.TraceOn(document.getElementById("txtTraceFile").value, sTraceOption, sTraceAll);
					UpdateStatus("Started trace.");
					document.getElementById("btnTrace").value = "Trace Off";
				}
				else if (document.getElementById("btnTrace").value == "Trace Off" && oSiebel != null){
					oSiebel.TraceOff();
					UpdateStatus("Stopped trace.");
					document.getElementById("btnTrace").value = "Trace On";	
				}
			}
			
		}
		
		/*
		--------------------------------------------------------------
		function: 	UpdateStatus
		arguments: 	string
		returns:	none
		desc.:		Displays the input message on UI.
		--------------------------------------------------------------
		*/
		function UpdateStatus(sMessage) {
			if (sMessage == "clear") sMessage = "";
			else var Timer = window.setTimeout("UpdateStatus('clear')", 5000);  // 5000 milliseconds = 5 seconds
			document.getElementById("txtStatus").innerHTML = sMessage;
		}
		
		/*
		--------------------------------------------------------------
		function: 	CheckSiebelExe
		arguments: 	none
		returns:	true/false
		desc.:		Returns true if siebel.exe is active (found 
					running on the machine). Else, returns false.
		--------------------------------------------------------------
		*/
		function CheckSiebelExe() {
			var sProcessString = "";
			var bSiebelRunning;
			var oWmgtRoot = GetObject("winmgmts:\\\\.\\root\\cimv2")
			var oWmgtItems= oWmgtRoot.ExecQuery("SELECT * FROM Win32_Process where Name='siebel.exe'")
			for (var enumItems=new Enumerator(oWmgtItems); !enumItems.atEnd(); enumItems.moveNext()) {
				sProcessString += enumItems.item().Name;
			}
			if (sProcessString.indexOf('siebel.exe') >= 0) bSiebelRunning = true;
			else bSiebelRunning = false;
			return bSiebelRunning;
    	}
		
		/*
		--------------------------------------------------------------
		function: 	ExitForm
		arguments: 	none
		returns:	true/false
		desc.:		Exit the application.
		--------------------------------------------------------------
		*/
		function ExitForm() {
			this.close();
		}
		
		/*
		--------------------------------------------------------------
		function: 	RunShellCommand
		arguments: 	file name
		returns:	true/false
		desc.:		Execute the given shell command.
		--------------------------------------------------------------
		*/
		function RunShellCommand(sCommand, sParam) {
			try {
				var oShell = new ActiveXObject("Shell.Application");
				oShell.ShellExecute(sCommand, sParam, "", "open", "1");
			}
			catch(e) {
				throw(e);
			}
			finally {
				oShell = "";
			}
		}
		
		/*
		--------------------------------------------------------------
		function: 	RunSiebelCommand
		arguments: 	string, comma delimited parameters
		returns:	none
		desc.:		Runs specified Siebel command with the provided
					params.
		--------------------------------------------------------------
		*/
		function RunSiebelCommand(sResponseField, sCommand, sParam) {
			var aParam = sParam.split(",");
			var sVal = "";
			for (var iTemp = 1; iTemp <= aParam.length; iTemp++){
				if (aParam[iTemp-1] != "") aParam[iTemp-1] = "'" + aParam[iTemp-1] + "'";
			}

			if (SetSiebelObject()) {
			
				switch (aParam.length) {
					case 1:
						sVal = eval("oSiebel." + sCommand + "(" + aParam [0] + ")");
						break;
					case 2:
						sVal = eval("oSiebel." + sCommand + "(" + aParam [0] + "," +  aParam[1] + ")");
						break;
					default:
						alert ("No. of parameters for the Siebel call are " + aParam.length + ", which is not supported by this program yet.");
				}
			} //if 
			
			if (sVal == "") sVal = "<blank>";
			
			if (sResponseField != "") document.getElementById(sResponseField).value = sVal;

			return sVal;
		}

		function SetBasicInfo(){
			document.getElementById("txtLogin").value = RunSiebelCommand("", "LoginName","");
			document.getElementById("txtActivePosition").value = RunSiebelCommand("", "PositionName","");
		}

	</script>
	
