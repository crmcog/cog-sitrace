			<div id="twocolumn"> 
				<div id="centercolumn">
					<fieldset class="field-set">
						<legend>Trace</legend>
						
						<label for="txtTraceFile">Trace File</label>
						<input type="text" class="CustomTextBox" value="c:\trace.txt" id="txtTraceFile" title="Name of the file that will have the output of the trace. Note that there will be 'custom' output only if you have trace statements within Siebel."/>
						<input type="button" class="button-small" value=" O " title="Open trace file." onClick="RunShellCommand(document.getElementById('txtTraceFile').value,'')"/>
						<p> 
						<label for="optTrace">Trace Option</label>
						<input type="radio" name="optTrace" value="Allocation" checked title="Turn tracing on using the 'Allocation' mode. This will trace all trace statements and Siebel allocation/deallocations.">Allocation</input>
						<input type="radio" name="optTrace" value="SQL" title="Trace for SQL statements."/>SQL<BR>
						<p>
						<div class="field-set-2row">
							<input type="button" value="Trace On" name="btnTrace"  class="button" onClick="ChangeTrace()" title="Turn trace on or off. Needs Siebel executable to be running"/> 
							<input type="button" value="Exit" name="btnExit"  class="button" onClick="ExitForm()" title="Exit the application."/><p> 
							
						</div>
					</fieldset>
					
					<fieldset class="field-set">
						<legend>Profile</legend>
						<div>
							<label for="ProfileName">Profile</label>
							<input type="text" class="CustomTextBox" value="" id="ProfileName" title="Profile attribute name to get/set."/>
							
						</div>
						<div>
							<label for="ProfileValue">Value</label>
							<input type="text" class="CustomTextBox" id="ProfileValue" title="Profile attribute value that is retrieved from application, or that will be set within the application."/>
						</div>
						<p>
						<div class="field-set-2row">
							<input type="button" value="GetProfileAttr" name="btnGetProfileAttr"  class="button" onClick="RunSiebelCommand('ProfileValue', 'GetProfileAttr', document.getElementById('ProfileName').value);" title="Get the profile attribute value for the specified name from Siebel application."/>
							<input type="button" value="SetProfileAttr" name="btnSetProfileAttr"  class="button" onClick="RunSiebelCommand('ProfileValue', 'SetProfileAttr', document.getElementById('ProfileName').value + ',' + document.getElementById('ProfileValue').value);" title="Set the profile attribute value for the specified name in Siebel application. The resulting value is displayed in 'Value'."/>
						</div>
					</fieldset>
					
					<fieldset class="field-set">
						<legend>Info.</legend>
						<div>
							<label for="txtLogin">Login</label>
							<input type="text"  class="CustomTextBox" value="" name="txtLogin" readonly title="Login id used in Siebel, read-only field here."/>
						</div>
						<div>
							<label for="txtActivePosition">Active Position</label>
							<input type="text"  class="CustomTextBox" value="" name="txtActivePosition" readonly title="Position id of the logged in user in Siebel, read-only field here."/>
						</div>
					</fieldset>
				</div>
				
				<div id="rightcolumn"></div>
			</div> 
			<div id="leftcolumn"></div>
		</div>
		<div id="footer-status">
				<div type="text" id="txtStatus"/>
		</div>
		<div class="push"></div>
